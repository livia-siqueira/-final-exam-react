{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "baseUrl": ".",
    "paths": {
      "@components/*": [ "./src/components/*"],
      "@storeUser/*": ["./src/store/users/controlUsers/*"],
      "@storeCart/*": ["./src/store/cartGames/*"],
      "@storeGames/*": ["./src/store/games/*"],
      "@pages/*": ["./src/pages/*"],
      "@utils/*": ["src/utils/*"],
      "@stylesGlobal/*": ["./src/stylesGlobal"],
    },
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ]
}
